{
	"extends": "../../tsconfig.base.json",
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"types": [],
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"noUncheckedIndexedAccess": false,
		"outDir": "dist",
		"rootDir": "src"
	}
}
